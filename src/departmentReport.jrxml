	<queryString>
		<![CDATA[SELECT
     DEPARTMENTS."DEPARTMENT_NAME" AS DEPARTMENTS_DEPARTMENT_NAME,
     EMPLOYEES."LAST_NAME" AS EMPLOYEES_LAST_NAME,
     LOCATIONS."CITY" AS LOCATIONS_CITY,
     DEPARTMENTS."DEPARTMENT_ID" AS DEPARTMENTS_DEPARTMENT_ID
